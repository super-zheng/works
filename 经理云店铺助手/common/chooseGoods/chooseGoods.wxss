.container{
  padding-top: 60px;
  box-sizing: border-box;
  font-size:32rpx;
}
.titleSearch{
  height: 60px;
  position: fixed;
  width: 100% ;
  top:0px;
  padding: 0px 60px;
  box-sizing: border-box;
  background-color: #fff;
}
.searchInput{
  border:1px solid #ccc;
  height: 40px;
  line-height: 40px;
  padding-left: 10px;
  border-radius: 5px;
  margin-top: 10px;
  padding-right: 50px;
  box-sizing: border-box;
}
.ewm image{
  width: 30px;
  height: 30px;
}
.ewm{
  position: absolute;
  left:0px;
  top: 0px;
  height: 60px;
  width:60px;
  justify-content: center;
  align-items: center;
  display: flex;
}
.titleSearch .section{
  position: relative;
}
.searchIcon{
  width: 20px;
  height: 20px;
  right: 0px;
  top:0px;
  padding: 10px;
  position: absolute;
}
.saixuan{
  position: absolute;
  width: 60px;
  height: 60px;
  right: 0px;
  top:0px;
  line-height: 60px;
  color:orange;
  text-align: center;
}
.saixuan image{
  width: 16px;
  height: 18px;
  vertical-align: middle;
}
.content{
  display:flex;
  width:100%;
  margin: 0;
  padding: 0;

}
.goodsLists{
  display: flex;
  width: 100%;
  overflow-x: auto;
  justify-content: space-between;
  align-items: baseline;
	flex-flow: row wrap;
}
.goodCard{
  width:50%;
  /* height: 200px; */
  display: inline-block;
  box-sizing: border-box;
  justify-content: center;
  align-items: center;
  padding: 5px;
}
.goodsBox{
  background-color:#fbfbfb;
  border:1px solid #eee;
}
.imgBox{
  height: 120px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.imgBox image{
  max-width: 100%;
  height: 100%;
}
.info{
  margin-top: 10px;
  font-size: 14px;
  padding-left: 5px;
  color:#777;
}
.check{
  border-color: #25a4ff;
}
.siderChoose{
  position: fixed;
  width: 100%;
  height: 100%;
  top:0px;
  background-color: rgba(0, 0, 0, .1);
  z-index: 99;
}
.chooseContent{
  position: absolute;
  right: 0px;
  height: 100%;
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  padding-top: 181px; 
  padding-bottom: 60px;
}
.goodsType{
  position: absolute;
  top:0px;
  left: 0px;
  width: 100%;
  height: 150px;
  border-bottom: 1px solid #ccc;
}
.lable{
  padding-left: 10px;
  height: 30px;
  line-height: 30px;
}
.types{
  display: flex;
  justify-content: space-between;
	flex-flow: row wrap;
  padding:0px 10px;
}
.typeBtn{
  width: 144rpx;
  height: 72rpx;
  background-color:rgba(0, 0, 0, .1);
  text-align: center;
  font-size:32rpx;
  line-height: 72rpx;
  margin-bottom: 10px;
  color:#777;
}
.typeBtnCheck{
  background-color: #25a4ff;
  color:#fff;
}
.endButton{
  position: absolute;
  bottom: 0;
  left: 0;
  height: 60px;
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.btn{
  width: 80px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  background-color: rgba(0, 0, 0, .1);
  color:#777;
  border-radius: 5px;
}
.btn.yes{
  background-color: #25a4ff;
  color:white;
}
.detailContent{
  width: 100%;
  height: 100%;
  display: flex;
}
.bigClass{
  width:181rpx;
  height: 100%;
}
.detail{
  height: 100%;
  flex: 1;
  background-color: #fbfbfb;
  overflow-y: auto;
  box-sizing: border-box;
}
.bigType{
  border:1px solid #ccc;
  background-color: rgba(0, 0, 0, .1);
  color:#777;
  width: 144rpx;
  height: 72rpx;
  overflow:auto;
  text-align: center;
  line-height: 72rpx;
  font-size: 32rpx;
  margin:10px auto;
  border-radius: 3px;
}
/* 大类点击选中样式 */
.bigTypeCheck{
  background-color: #25a4ff;
  color:white;
}
.del{
  width: 80%;
  height: 72rpx;
  background-color: #eee;
  border-radius: 3px;
  color: #777;
  text-align: center;
  line-height: 72rpx;
  margin: 10px auto;
}
/* 详细信息点击选中样式 */
.detailCheck{
  background-color: #25a4ff;
  color:white;
}

/*购物车样式  */
.shopCar{
  width: 80px;
  height: 80px;
  position: fixed;
  bottom: 20px;
  right: 20px;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, .6);
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.shopCar text{
  position: absolute;
  width:30px;
  height: 30px;
  text-align: center;
  line-height: 30px;
  color:white;
  right: 10px;
  top:10px;
  font-size: 12px;
  border-radius: 50%;
  background-color: red;
}
.scal{
  transform: scale(.8,.8);
}

/*球柱镜选择弹框样式  */
.chooseQZ{
  display: flex;
  background-color: #fff;
  justify-content: space-around;
}
.pickerView{
  width:300rpx;
}
.pickerView picker-view{
  margin: 0 auto;
}
.pickerTitle{
  text-align: center;
  margin: 20rpx 0;
}