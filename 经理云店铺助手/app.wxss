/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.fl{
  float: left;
}
.fr{
  float: right;
}
.no_products{
  height: 40px;
  text-align: center;
  line-height: 40px;
  color: red;
  font-size: 16px;
  margin: 20px 0px;
}
.radioUncheck{
  display: inline-block;
  width: 1rem;
  height: 1rem;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,)
}
.radioCheck{
  background-image: url(data:image/png;base64,);
}
.btnActive{
  transform: scale(1.1);
}
.color777{
  color:#777;
}

.colorblue{
  color:#0094ff;
}

.colorOrange{
  color:orange;
}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}
.clearfix { zoom: 1 }