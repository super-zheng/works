page{
  height: 100%;
  padding: 0;
}
.container{
  height: 100%;
  width: 100%;
  padding: 0px;
  margin: 0;
}
.content{
  flex: 1;
  width: 100%;
  height: 100%;
  overflow: auto;
  background-color:#eee;
}
.footbar{
  width:100%;
  height: 3rem;
  background-color: white;
}
.list{
  height: 5rem;
  display: flex;
  flex-flow: row;
  background-color:white;
  margin-bottom: .1rem;
}
.leftBox{
  width: 7.5rem;
  height: 5rem;
  margin: 0;
  padding: 0;
  display: flex;
  flex-flow:row;
}
.checkBtn{
  width:2.5rem;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.imgBox{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.imgBox image{
 max-width:5rem;
 max-height: 5rem;
}
.uncheck{
  width: 1.4rem;
  height: 1.4rem;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,)
}
.check{
  background-image: url(data:image/png;base64,);
}
.rightBox{
  display: flex;
  flex: 1;
  height: 100%;
}
.showMessage,.func{
  height: 100%;
  width: 100%;
  display: flex;
}
.detail{
  height: 100%;
  flex: 1;
  padding: .5rem ;
  font-size:.9rem;
  box-sizing: border-box;
  display: flex;
  flex-flow: column;
}
.detail .message{
  flex: 1;
}
.edit{
  width: 2rem;
  height: 100%;
  display: flex;
  flex-flow: column;
  justify-content: space-around;
  align-items: center;
  font-size: .7rem;
  color:#777;
}
.edit image{
  height: 1rem;
  width: 1rem;
}
.kucun{
  height: 1rem;
  width: 100%;
  display: flex;
  color:orange;
  vertical-align: bottom;
  justify-content: space-between;
}
.stockAdd{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.rightBtns{
  width: 2.5rem;
  height: 100%;
}
.succ{
  font-size: .8rem;
  width: 100%;
  height: 100%;
  line-height: 5rem;
  text-align: center;
  color:white;
}
.succ{
  background-color: #23a8e0;
}
.stockBox{
  width:6rem;
  height: 2.5rem;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.num{
  border:1px solid #ccc;
  width: 2rem;
  padding-left: .4rem;
  font-size: .8rem;
}
.reduce,.add{
  width: 1.25rem;
  height: 1.25rem;
  border-radius: .2rem;
  background-color: #23a8e0;
  position: relative;
}
.reduce::before,.add::before{
  content: '';
  width: .75rem;
  height: .1rem;
  background-color: white;
  position: absolute;
  left: .25rem;
  top:.575rem;
}
.add::after{
  content: '';
  width: .1rem;
  height: .75rem;
  position: absolute;
  left: .575rem;
  top:.25rem;
  background-color: white;
}
.footbar{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.checkAllText {
  display: block;
  text-align: center;
  font-size: .8rem;
}
.del,.toOrder{
  display: inline-block;
  height: 2rem;
  width: 3rem;
  background-color:#23a8e0;
  font-size: .9rem;
  text-align: center;
  line-height: 2rem;
  color:white;
  border-radius: .2rem;
}
.del{
  background-color: #ffa550;
}
.endBox{
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right: 1rem;
}
.colorRed{
  font-weight: 900;
  color:red;
}