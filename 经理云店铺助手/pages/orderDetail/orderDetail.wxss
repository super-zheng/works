.content {
  padding-bottom: 30rpx;
  box-sizing: border-box;
  font-family: Arial;
  background: #f5f5f5;
  font-size: 32rpx;
}

.header {
  height: 100rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx 0 20rpx;
}

.header image {
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
}

.p1 {
  flex: 1;
  padding-left: 8px;
}

.xiao {
  width: 52rpx;
  height: 52rpx;
  border-radius: 50%;
  border: 1px solid #4ddf46;
  color: #4ddf46;
  text-align: center;
  line-height: 52rpx;
}

.ordernum {
  padding-right: 10rpx;
  display: flex;
  justify-content: space-between;
}

.processing {
  color: #fc3433;
  width: 86rpx;
  height: 46rpx;
  line-height: 46rpx;
  white-space: nowrap;
}

.detail {
  background: #fff;
  margin-top: 15rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}

.people {
  display: flex;
  margin-bottom: 8rpx;
}

.people {
  display: flex;
  margin-bottom: 16rpx;
}

.cashier {
  margin-left: 90rpx;
}

.goods {
  margin-top: 10rpx;
}

.pic {
  width: 100%;
  height: 100%;
}

.picbox {
  width: 216rpx;
  height: 200rpx;
  position: relative;
}

.goods-name {
  width: 30%;
  text-align: center;
  color: #fff;
  background: rgba(0, 95, 138, 0.4);
  padding: 0 6px;
  position: absolute;
  bottom: 0;
  left: 0;
}

.goods-item {
  display: flex;
  position: relative;
  margin-bottom: 10rpx;
  background: #fff;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}

.goods-detail {
  margin-left: 30rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.price {
  display: flex;
  justify-content: space-between;
  padding-right: 44rpx;
  margin-top: 20rpx;
}

.money {
  color: #fb5824;
}

.more {
  position: absolute;
  right: 24rpx;
  top: 110rpx;
}

.discount {
  color: #fc3433;
  /*width: 43px;*/
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
}

.pinpai {
  display: flex;
  justify-content: space-between;
  padding-right: 10rpx;
}

.should {
  color: #fe7f58;
}

.weishou {
  color: #fc3433;
}

.pd2 text:nth-child(2) {
  color: #5de356;
}

/* .pd2 text {
  margin-left: 25rpx;
} */

.zero {
  padding-right: 20rpx;
}

.price-detail {
  color: #737373;
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.price-detail text{
  min-width: 120rpx;
}


.pd2 {
  display: flex;
  justify-content: flex-start; 
}
.pd2 text:not(:first-child){
  margin-left: 25rpx;
}


.price-box {
  background: #fff;
  padding: 10rpx 0rpx 5px 25rpx;
  box-sizing: border-box; 
}

/* 结算方式 */

.setelement {
  background: #fff;
  padding: 0 20rpx 40rpx 20rpx;
}

.cash {
  display: flex;
  justify-content: space-between;
  margin-top: 14rpx;
  color: #737373;
  padding-right: 10rpx;
  box-sizing: border-box;
}
.cash text{
  min-width: 250rpx;
}

/* .cash-p1 {
  padding-left: 20rpx;
} */

.alipay {
  padding-bottom: 10rpx;
}

.jiesuan {
  font-weight: bold;
  font-size: 30rpx;
}

.jiesuan::after {
  content: "";
  display: inline-block;
  width: 80%;
  border: 1px solid #d9d9d9;
  margin-left: 20rpx;
  margin-bottom: 10rpx;
}

/* 验光单号 */

.optometry {
  padding-bottom: 40rpx;
  margin-top: 8rpx;
  box-sizing: border-box;
  background: #fff;
}

.single-number {
  display: flex;
  justify-content: space-between;
  margin-top: 16rpx;
  padding: 20rpx 20rpx 0 20rpx;
  box-sizing: border-box;
}

.single-number text {
  font-size: 35rpx;
}

.completed {
  color: #67e361;
  line-height: 46rpx;
  text-align: center;
}

.avatar {
  width: 132rpx;
  height: 132rpx;
  border-radius: 50%;
}

.vip-detail {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  margin-top: 10rpx;
}

.infor {
  flex: 1;
  padding: 0 10rpx 0 10rpx;
  font-size: 32rpx;
}

.name, .beizhu {
  display: flex;
  justify-content: space-between;
}

.beizhu {
  margin-top: 12rpx;
  justify-content: flex-start;
}

.jifen {
  margin-left: 60rpx;
}

.beizhu-input {
  display: flex;
  justify-content: space-around;
}

.input {
  width: 260rpx;
  height: 70rpx;
  font-size: 24rpx;
  border: 1px solid #bbb;
}

.time-picker {
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 40rpx;
  padding: 0 20rpx 0 10rpx;
  box-sizing: border-box;
}

