/* 表格 */

.table-container {
  margin-top: 32rpx;
}

.table, .table4 {
  width: 95%;
  margin: auto;
  border: 1px solid #dadada;
  border-right: 0;
  border-bottom: 0;
}

.tr {
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.th, .td {
  padding: 20rpx;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  /* white-space:nowrap;    */
}

.th {
  font-weight: 400;
  /* background-color: #dadada; *//* display: flex;
  align-items: center; */
}

.td1 {
  color: #a63158;
}

.td2 {
  color: #259b24;
}

.th4, .td4 {
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  width: 12%;
  padding: 10rpx;
  text-align: center;
}

.tr4 {
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.table4 .th4:nth-child(3), .table4 .td4:nth-child(3) {
  width: 17%;
}

.table4 .th4:nth-child(5), .table4 .td4:nth-child(5) {
  width: 17%;
}

.table4 .th4:first-child, .table4 .td4:first-child {
  flex: 1;
}

.table4 .tr4:nth-child(2) .td4,.table4 .tr4:nth-child(4) .td4,.table4 .tr4:nth-child(5) .td4, .table4 .tr4:nth-child(7) .td4{
  border-bottom: none;
 
}

