.searchBox {
  padding: 10px 20px;
  padding-right: 80px;
  position: relative;
  background-color: #fff;
}

.searchBtn {
  position: absolute;
  width: 60px;
  text-align: center;
  height: 38px;
  line-height: 38px;
  right: 10px;
  top: 10px;
  font-size: 18px;
  color: #43abde;
}

.activeSearch {
  background-color: #43abde;
  color: #fff;
}
.chooseCustomer{
  padding-right: 80px;
  position: relative;
  height: 100px;
  margin-top: 20px;
  background-color: #fff;
}
.addCustomer{
  position: absolute;
  right: 10px;
  top:0px;
  width: 60px;
  height: 100px;
  font-size: 14px;
  color:#aaa;
  text-align: center;
}
.addCustomer image{
  width: 40px;
  height: 40px;
}
.gukeIcon{
  width: 50px;
  height: 50px;
}
.guke{
  text-align: center;
  font-size: 14px;
  box-sizing: border-box;
  overflow: hidden;
}
.active{
  width:80px !important;
  transition: all .2s;
}
.active image{
  width:70px;
  height: 70px;
  transition: all .2s;
}
.func{
  height: 220px;
  display: flex;
  padding: 10px;
  margin-top: 10px;
	flex-flow: row wrap;
  justify-content:space-around ;
}
.cardBox{
  width:30%;
  height: 50%;
  background-color: #30a2de;
  margin-bottom: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.cardBoxL::before{
  content:'';
  display: block;
  width:100%;
  height: 100%;
}
.endBox{
  height: 0;
}
.iconText{
  height: 100%;
}
.iconText image{
  width: 75px;
  height: 75px;
}
.iconText view{
  width: 100%;
  font-size: 16px;
  color:#fff;
  height: 16px;
  line-height: 16px;
}
.footBar{
  height: 70px;
  position: fixed;
  bottom: 0px;
  left: 0px;
  width: 100%;
  justify-content: space-around;
  display: flex;
  background-color: red;
}
.navTo{
  width: 33.333%;
  height: 70px;
  background-color: #fff;
  justify-content: center;
  align-items: center;
  text-align: center;
}
.navbox image{
  width:40px;
  height: 40px;
}
.navbox {
  font-size: 14px;
  color:#ccc;
}