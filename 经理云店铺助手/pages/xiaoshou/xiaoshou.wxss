  background-color: #aaa;
}
.searchInput {
  border: 1px solid #ccc;
  height: 38px;
  line-height: 38px;
  border-radius: 5px;
  padding-left: 10px;
  padding-right: 40px;
  font-size: 18px;
  position: relative;
}
