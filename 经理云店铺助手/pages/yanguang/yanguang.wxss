.tab{
  height: 100rpx;
  width: 100%;
  display: flex;
  justify-content: space-around;
  background: #fff;
  margin-top:10rpx;
  padding-top:0.5rem;
  box-sizing: border-box;
}
.tab text{
  /* float: left; */
  font-size: 33rpx;
  font-weight: bold;
  font-family:"楷体";
  text-align: center;
  white-space:nowrap; 
  line-height: 80rpx;
  padding:0 10rpx;
  box-sizing: border-box;
  border-bottom: 12rpx solid #4398c2;
}
.tab text:nth-child(2){
  flex: 1;
}
.tab text:not(:nth-child(2)){
  width: 320rpx;
}
.bgcolor{
  background: #4398c2;
}

.dingpeiContent {
  font-size: 0.6rem;
}

.chooseRL {
  display: flex;
  margin-top: 0.5rem;
  margin-bottom: 0.5rem;
  justify-content: center;
}

.choseSmall {
  display: inline-block;
  border: 0.1rem solid #23a8e0;
  padding: 0.3rem 0.7rem;
  color: #23a8e0;
}

.choseSmall.left {
  border-left: none;
  border-top-right-radius: 0.3rem;
  border-bottom-right-radius: 0.3rem;
}

.choseSmall.right {
  border-right: none;
  border-top-left-radius: 0.3rem;
  border-bottom-left-radius: 0.3rem;
}

.choseSmall.RLactive {
  background-color: #23a8e0;
  color: white;
}

.dingpeidan {
  display: flex;
