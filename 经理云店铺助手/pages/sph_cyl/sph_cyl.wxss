.changeBtn{
  width:150rpx;
  height: 75rpx;
}
.funcBtns{
  display: flex;
  justify-content: space-around;
  padding: 20rpx 0;
  font-size:32rpx;

}
.endShow{
  display: flex;
  justify-content: space-between;
  font-size:32rpx;
}
.endShow .info{
  display: flex;
  flex-flow: column;
  align-items: center;
  width: 150rpx;
  font-size:32rpx;
  height: 50px;
  color:#777;
  justify-content: space-around;
}
.QZtd.green{
  color:green;
  font-weight: 900;
}
.QZtd.red{
  color:red;
  font-weight: 900;
}
.endBtn{
  position: fixed;
  bottom: 30rpx;
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.btn{
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 10rpx;
}
.requery{
  background-color: #26a9e1;
  color:white;
}
.cancel{
  border:1px solid #777;
  color:#777;
}
.QZtd.checktd{
  background-color: #777;
  color:white;
}
.saomaImg{
  width:100rpx;
  height:80rpx;
}
.header-1{
  background: #fff;
}
.header-2{
  background: #EEEEEE;
  color: #000;
}